{
  "name": "batr",
  "version": "0.0.1",
  "description": "Batr - Bundle and Test CommonJS + ESM in the browser and NodeJS. And repeat with Travis-CI.",
  "main": "dist/stopword.cjs.js",
  "module": "dist/stopword.esm.js",
  "browser": "dist/stopword.js",
  "scripts": {
    "build": "rollup --config",
    "empty-sandbox": "rm -rf test/sandbox && mkdir test/sandbox",
    "empty-demo": "rm -rf demo/",
    "copy-stopword": "cp -r ./node_modules/stopword/demo ./",
    "test": "npm run empty-sandbox && standard './*.js' './test/*.js' && npm run copy-stopword && npm run build && tape ./test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eklem/batr.git"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "playwright": "^1.10.0",
    "rollup": "^2.45.2",
    "standard": "^16.0.2",
    "tape": "^5.2.2"
  },
  "devDependencies": {
    "stopword": "^1.0.7"
  },
  "keywords": [
    "bundling",
    "testing",
    "nodejs",
    "browser"
  ],
  "author": "Espen Klem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eklem/batr/issues"
  },
  "homepage": "https://github.com/eklem/batr#readme"
}
