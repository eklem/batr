{
  "name": "batr",
  "version": "0.1.0",
  "description": "Batr - Bundle and Test CommonJS + ESM in the browser and NodeJS. And repeat with Travis-CI.",
  "main": "test/stopword/lib/stopword.cjs.js",
  "module": "test/stopword/lib/stopword.esm.js",
  "browser": "test/stopword/dist/stopword.js",
  "scripts": {
    "build": "rollup --config",
    "empty-sandbox": "rm -rf test/stopword/",
    "copy-stopword": "cp -r ./node_modules/stopword ./test",
    "test": "npm run empty-sandbox && npm run copy-stopword && npm run build && npx ava ./test/unit-tests.js && npx ava ./test/userinterface-tests.js && npm run empty-sandbox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eklem/batr.git"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "ava": "3.15.0",
    "playwright": "^1.10.0",
    "rollup": "^2.46.0",
    "standard": "^16.0.2"
  },
  "devDependencies": {
    "stopword": "^1.0.7"
  },
  "keywords": [
    "building",
    "testing",
    "bundling",
    "nodejs",
    "browser"
  ],
  "author": "Espen Klem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eklem/batr/issues"
  },
  "homepage": "https://github.com/eklem/batr#readme"
}
